 	relation SibsReportFileTreasuryFile {
 		
		TreasuryFile playsRole treasuryFile {
			multiplicity 1..1;
		}
 		
 		paymentcodes.SibsReportFileDomainObject playsRole {
 			multiplicity 0..1;
 		}
 	}
 	
